{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*).html(.*)",
            "dest": "publish/$1"
        },
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
      ]
}